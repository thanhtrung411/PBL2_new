                            <property name="styleSheet">
                             <string notr="true">/* --- PHẦN 1: Ô NHẬP LIỆU (Giống ComboBox) --- */
QDateEdit {
    border: 2px solid transparent;
    border-radius: 8px;
    padding: 6px 12px;
    background-color: #F3F4F6; /* Nền xám nhạt */
    color: #333;
    selection-background-color: rgb(69, 104, 173);
}

/* Khi di chuột vào */
QDateEdit:hover {
    background-color: #E5E7EB;
}

/* Khi đang nhập liệu (Focus) */
QDateEdit:focus {
    background-color: white;
    border: 2px solid rgb(69, 104, 173); /* Viền xanh */
}

/* Nút mũi tên xổ xuống */
QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 30px;
    border-left-width: 0px;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
}

/* Icon (Bạn có thể dùng icon lịch 'calendar.png' thay vì mũi tên cho đúng điệu) */
QDateEdit::down-arrow {
    image: url(:/icons/icons_/down-chevron.png); /* Hoặc file icon lịch của bạn */
    width: 16px;
    height: 16px;
}

/* --- PHẦN 2: LỊCH XỔ XUỐNG (QCalendarWidget) --- */

/* Khung tổng thể của lịch */
QCalendarWidget QWidget {
    alternate-background-color: #f9f9f9; 
    background-color: white;
    border-bottom-right-radius: 8px;
    border-bottom-left-radius: 8px;
}

/* Thanh điều hướng phía trên (Tháng/Năm) */
QCalendarWidget QToolButton {
    color: #333;
    background-color: transparent;
    border: none;
    font-weight: bold;
    icon-size: 20px; /* Chỉnh kích thước mũi tên trái phải của lịch */
}
QCalendarWidget QToolButton:hover {
    background-color: #F3F4F6;
    border-radius: 4px;
}

/* Menu chọn tháng năm khi bấm vào tiêu đề */
QCalendarWidget QMenu {
    background-color: white;
    border: 1px solid #ccc;
}

/* Bảng ngày tháng (Spin view) */
QCalendarWidget QAbstractItemView:enabled {
    font-size: 13px;
    color: #333;
    background-color: white;
    selection-background-color: rgb(69, 104, 173); /* Màu nền ngày được chọn */
    selection-color: white; /* Màu chữ ngày được chọn */
    outline: 0; /* Bỏ đường viền nét đứt xấu xí */
}

/* Ngày trong tuần (T2, T3, T4...) */
QCalendarWidget QAbstractItemView:disabled {
    color: #888; /* Màu chữ xám cho ngày không thuộc tháng hiện tại */
}</string>
