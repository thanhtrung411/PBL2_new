     <widget class="QStackedWidget" name="noi_dung_layout">
      <property name="styleSheet">
       <string notr="true">/* 1. TỔNG QUAN: Font chữ và nền chính */
QWidget {
    background-color: #FFFFFF; /* Nền trắng tinh khôi */
    color: #2c3e50; /* Chữ màu xám đậm (không dùng đen xì) cho dịu mắt */
    font-family: &quot;Segoe UI&quot;, sans-serif;
}

/* Nền của cửa sổ chính có thể cho xám cực nhạt để làm nổi bật các ô nhập liệu */
QMainWindow {
    background-color: #f8f9fa; 
}

/* 2. Ô NHẬP LIỆU: Tròn trịa, sáng sủa */
QLineEdit, QSpinBox, QDateEdit, QComboBox {
    background-color: #FFFFFF;
    border: 1px solid #e0e0e0; /* Viền xám rất mờ */
    border-radius: 8px; /* Bo tròn nhiều hơn cho mềm mại */
    padding: 8px 12px; /* Khoảng cách chữ rộng rãi */
    selection-background-color: #3498db;
}

/* Khi bấm vào thì sáng lên màu xanh ngọc */
QLineEdit:focus, QSpinBox:focus, QDateEdit:focus, QComboBox:focus {
    border: 2px solid #5dade2; /* Màu xanh sáng */
    background-color: #fbfdff;
}

/* 3. NÚT BẤM: Tươi tắn */
QPushButton {
    background-color: #3498db; /* Xanh dương tươi (Bright Blue) */
    color: white;
    border: none;
    border-radius: 8px;
    padding: 10px 20px;
    font-weight: bold;
    font-size: 14px;
}

QPushButton:hover {
    background-color: #5dade2; /* Sáng hơn khi di chuột */
}

QPushButton:pressed {
    background-color: #2980b9; /* Tối hơn chút khi ấn */
    padding-top: 11px;
}

/* Nút phụ (ví dụ nút Hủy) cho màu xám nhạt */
QPushButton#btnCancel {
    background-color: #ecf0f1;
    color: #7f8c8d;
}
QPushButton#btnCancel:hover {
    background-color: #bdc3c7;
}

/* 4. BẢNG DỮ LIỆU (TABLE): Thoáng, không kẻ ô chằng chịt */
QTableWidget {
    background-color: #FFFFFF;
    border: 1px solid #ecf0f1;
    border-radius: 8px;
    gridline-color: transparent; /* Ẩn đường kẻ lưới dọc/ngang đi cho thoáng */
    selection-background-color: #ebf5fb; /* Màu nền khi chọn dòng (Xanh cực nhạt) */
    selection-color: #2980b9; /* Màu chữ khi chọn */
    outline: 0; /* Bỏ đường viền nét đứt khi chọn */
}

/* Tiêu đề bảng */
QHeaderView::section {
    background-color: #FFFFFF;
    color: #7f8c8d; /* Chữ tiêu đề màu xám */
    padding: 10px;
    border: none;
    border-bottom: 2px solid #ecf0f1; /* Chỉ kẻ gạch chân dưới tiêu đề */
    font-weight: bold;
    text-transform: uppercase; /* Chữ in hoa */
}

/* 5. GROUP BOX (Khung nhóm) */
QGroupBox {
    border: 1px solid #e0e0e0;
    border-radius: 8px;
    margin-top: 20px;
    font-weight: bold;
    color: #3498db; /* Tiêu đề group màu xanh */
}
QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 5px;
    background-color: #f8f9fa; /* Nền tiêu đề trùng nền cửa sổ */
}</string>
