                <property name="styleSheet">
                 <string notr="true">/* 1. Style cho bản thân cái ComboBox */
QComboBox {
    border: 1px solid #4568ad;
    border-radius: 8px;
    padding: 6px 12px;
    background-color: white;
    color: #4568ad;
    font-size: 14px; /* Thêm size chữ cho dễ đọc */
}

QComboBox:hover {
    border-color: #2a4a8b; /* Đổi màu đậm hơn chút khi hover */
    background-color: #f9faff;
}

/* Khi đang mở danh sách -&gt; Vuông 2 góc dưới để nối với danh sách */
QComboBox:on {
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
    border-bottom: 1px solid #f0f0f0; /* Làm mờ cạnh dưới để cảm giác liền mạch */
}

/* Mũi tên */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 30px;
    border-left-width: 0px;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
}

QComboBox::down-arrow {
    image: url(:/icons/icons_/down-chevron.png); 
    width: 16px;
    height: 16px;
}

/* 2. Style cho cái Danh sách xổ xuống (Quan trọng) */
QComboBox QAbstractItemView {
    border: 1px solid #4568ad; /* Đã thêm dấu # */
    border-top: none;          /* Bỏ viền trên để nối với cái box */
    
    /* Vuông 2 góc trên, bo 2 góc dưới */
    border-top-left-radius: 0px;
    border-top-right-radius: 0px;
    border-bottom-left-radius: 8px;
    border-bottom-right-radius: 8px;
    
    background-color: white;
    color: #4568ad;
    
    outline: 0px;  /* QUAN TRỌNG: Dòng này xóa cái viền nét đứt xấu xí */
    selection-background-color: #4568ad; /* Màu nền khi chọn */
    selection-color: white;              /* Màu chữ khi chọn */
}

/* Từng dòng trong danh sách */
QComboBox QAbstractItemView::item {
    height: 30px; /* Tăng chiều cao cho thoáng */
    padding-left: 10px; /* Cách lề trái */
    margin: 0px;
}

/* Khi di chuột vào item (nhưng chưa click) */
QComboBox QAbstractItemView::item:hover {
    background-color: #f0f5ff;
    color: #4568ad;
    border: none; /* Đảm bảo không có viền lạ */
}

/* Khi item đã được chọn */
QComboBox QAbstractItemView::item:selected {
    background-color: #4568ad;
    color: white;
    border: none;
}</string>
